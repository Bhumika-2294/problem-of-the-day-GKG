class Solution {
  public:
    int findMoves(vector<int>& chairs, vector<int>& passengers) {
        // Sorting both the arrays
        sort(chairs.begin(), chairs.end());
        sort(passengers.begin(), passengers.end());
        int ans = 0;
        int n = chairs.size();
        // Calculating minimum
        // position changed
        for (int i = 0; i < n; i++) {
            ans += abs(chairs[i] - passengers[i]);
        }

        return ans;
    }
};
